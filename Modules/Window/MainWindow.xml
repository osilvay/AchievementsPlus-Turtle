        this:SetBackdropBorderColor(0.91, 0.91, 0.91, 0.8)
      </OnLoad>
      <OnMouseUp>
        if ( this.isMoving ) then
          this:StopMovingOrSizing()
          this.isMoving = false
        end
      </OnMouseUp>
      <OnMouseDown>
        if ( ( not this.isLocked ) or ( this.isLocked == 0 ) ) and ( arg1 == "LeftButton" ) then
          this:StartMoving()
          this.isMoving = true
        end
      </OnMouseDown>
